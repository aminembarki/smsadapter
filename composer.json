        "prawnsalad/nexmo": "dev-master",
        "plivo/plivo-php": "dev-master",
        "twilio/sdk": "~3.12",
        "arcturial/clickatell": "~2.0"
    },
    "autoload": {
        "psr-0": {"Smsadapter": "src/"},
        "classmap": [
